		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Covid-Reporting-Euro-adf"
		},
		"cases_deaths_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'cases_deaths'"
		},
		"hospital_daily_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'hospital_daily'"
		},
		"ls_ablob_covid_dl_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_ablob_covid_dl'"
		},
		"ls_ablob_covid_sa_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablob_covid_sa'"
		},
		"ls_ablob_covid_dl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covidreportingdl1995.dfs.core.windows.net"
		},
		"ls_http_ecdc_data_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().sourceBaseURL}"
		}
	},
